<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="cn.springAOP.staticProxy.EmployeeDAOImp" id="employeeDAOImp"/>
    <context:component-scan base-package="cn.springAOP.staticProxy"/>
    <context:annotation-config/>
    <!--<bean class="cn.springAOP.staticProxy.service.EmployeeServiceImp" id="employeeServiceImp"/>-->

    <bean class="cn.springAOP.staticProxy.Manager" id="manager"/>
    
    <bean id="employeeServiceProxy" class="cn.springAOP.staticProxy.EmployeeServiceProxy">
        <property name="target" ref="employeeDAOImp"/>
        <property name="manager" ref="manager"/>
    </bean>
    

</beans>